{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["@react-native"],
  "rules": {
    "no-console": 1,
    "prettier/prettier": [
      1,
      {
        "semi": true,
        "tabWidth": 2,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 80,
        "endOfLine": "auto"
      }
    ]
  }
}