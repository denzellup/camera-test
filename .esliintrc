{
  "root": true,
  "parser": "@babel/eslint-parser",
  "plugins": [
    "react",
    "react-native",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "no-console": 1,
    "prettier/prettier": [
      1,
      {
        "semi": true,
        "tabWidth": 2,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 80,
        "endOfLine": "auto"
      }
    ],
    "react/prop-types": [
      "error",
      {
        "ignore": [
          "navigation",
          "navigation.navigate"
        ]
      }
    ]
  }
}
